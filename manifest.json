{
  "name": "KriKCup",
  "description": "Fetches the scores from cricinfo and displays it to user.",
  "version": "1.0",
  "permissions": [
    "tabs", "http://*/*", "https://*/*"
  ],
  "browser_action": {
      "default_title": "Show Live Cricket Score!",
      "default_icon": "images/icon.png",
      "default_popup": "html/index.html"
  },
  "manifest_version": 2
}
